{
  "name": "@travi/hapi-react-router",
  "license": "MIT",
  "description": "hapi route to delegate routing for html content to react-router",
  "version": "0.1.0",
  "main": "lib/plugin.cjs.js",
  "module": "lib/plugin.es.js",
  "scripts": {
    "lint:js": "eslint .",
    "tests:unit": "mocha --recursive test/unit",
    "test": "run-s lint:* tests:*",
    "start": "babel-node ./example/app.js",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/travi/hapi-react-router.git"
  },
  "keywords": [
    "hapi",
    "react",
    "react-router",
    "server-side render",
    "ssr",
    "redux"
  ],
  "author": "Matt Travi <npm@travi.org> (https://matt.travi.org/)",
  "bugs": {
    "url": "https://github.com/travi/hapi-react-router/issues"
  },
  "homepage": "https://github.com/travi/hapi-react-router#readme",
  "devDependencies": {
    "@travi/any": "0.7.0",
    "@travi/eslint-config-travi": "0.3.0",
    "@travi/hapi-html-request-router": "0.1.0",
    "babel-cli": "6.18.0",
    "babel-preset-es2015-node": "6.1.1",
    "chai": "3.5.0",
    "cz-conventional-changelog": "1.2.0",
    "eslint": "3.10.0",
    "glue": "4.0.0",
    "good": "7.0.2",
    "good-console": "6.3.1",
    "good-squeeze": "5.0.0",
    "hapi-mustache": "0.0.1",
    "husky": "0.11.9",
    "mocha": "3.1.2",
    "npm-run-all": "3.1.1",
    "sinon": "1.17.6",
    "validate-commit-msg": "2.8.2",
    "vision": "4.1.0",
    "visionary": "6.0.2"
  },
  "optionalDependencies": {
    "@travi/hapi-html-request-router": "*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
